<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.p3p20.model.licence" table="licence">
		<id name="idLicence" type="int" column="idLicence">
			<generator class="increment" />
		</id>
		<property name="nomLicence">
			<column name="nomLicence" />
		</property>
		<property name="typeLicence">
			<column name="typeLicence" />
		</property>
	</class>
</hibernate-mapping>